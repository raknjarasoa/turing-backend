{
  "name": "turing-backend",
  "version": "1.0.0",
  "description": "Turing fullstack challenge",
  "keywords": [
    "prisma",
    "graphql",
    "nodejs",
    "typescript",
    "babel",
    "jest",
    "prettier",
    "eslint",
    "ci/cd"
  ],
  "author": "Njarasoa Rakotozafy",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "npm-run-all --parallel tsc:w debug",
    "debug": "nodemon -e js,ts,graphql -x babel-node --inspect dist/index.js",
    "play": "npm-run-all --parallel debug playground",
    "playground": "graphql playground --dotenv .env",
    "build": "rm -rf dist && graphql codegen && babel --extensions '.ts' src -d dist",
    "tsc": "tsc",
    "tsc:w": "tsc --watch",
    "test": "jest --env=node",
    "test:w": "jest --watch --env=node",
    "test:ci": "jest --runInBand --env=node",
    "test:cov": "jest --coverage --env=node",
    "codecov": "codecov -f coverage/*.json",
    "lint": "eslint src/**.ts",
    "format": "prettier './{src,__test__,}/**/*.{js,ts,json,md,graphql}' --write",
    "deploy": "rm -rf src/generated-prisma-client && npx prisma deploy && yarn format"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.5.0",
    "nodemailer": "^5.1.1",
    "prisma-client-lib": "^1.27.4",
    "stripe": "^6.25.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^6.1.0",
    "@types/jest": "^24.0.9",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/node": "^11.10.4",
    "@types/nodemailer": "^4.6.6",
    "@types/stripe": "^6.19.8",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-jest": "^24.1.0",
    "codecov": "^3.2.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.15.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "graphql": "^14.1.1",
    "graphql-cli": "^3.0.11",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "prisma": "^1.27.4",
    "typescript": "^3.3.3333"
  },
  "repository": {
    "url": "http://raknjarasoa.me"
  }
}
